{
  "name": "joystreamclassic",
  "productName": "JoystreamClassic",
  "version": "1.0.8",
  "description": "A blockchain powered decentralized media distribution, curation, discovery and monetization platform.",
  "main": "src/main.js",
  "scripts": {
    "start": "env JOYSTREAM_NODE_LOGS=1 electron-forge start",
    "component-development": "env COMPONENT_DEVELOPMENT_MODE=True NODE_ENV=development electron-forge start",
    "force-onboarding": "env FORCE_ONBOARDING=True NODE_ENV=development electron-forge start",
    "force-terms-screen": "env FORCE_TERMS_SCREEN=True NODE_ENV=development electron-forge start",
    "auto-update": "env AUTO_UPDATE=True NODE_ENV=development electron-forge start",
    "mobx-dev": "env MOBX_DEV=True NODE_ENV=development electron-forge start",
    "test": "xvfb-maybe electron-mocha renderer --preload test/preload-polyfill.js --recursive ./test",
    "test-file": "xvfb-maybe electron-mocha renderer ",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "rebuild": "electron-rebuild",
    "publish": "electron-forge publish"
  },
  "keywords": [
    "joystreamclassic",
    "bitcoin",
    "desktop",
    "electron",
    "torrent",
    "torrent-client"
  ],
  "author": {
